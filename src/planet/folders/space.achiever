CREATE FOLDER Space (
	LANGUAGE english,
	NAME COLUMN SmallText,
	"Title" SmallText WITH Required=true,
	"Description" LongText,
	"Keywords" SmallText WITH IsSet=true,
    "Content" LongText,
    "Main Image" File,
    "Images" File WITH Many=true,
    "Meta" Object,
    "Parent" Link WITH LinkedFolder=Space,
);